{
  "name": "sakila-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run generate-clients && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-clients": "npm run generate-address-client && npm run generate-film-client && npm run generate-payment-client && npm run generate-store-client",
    "generate-address-client": "ng-swagger-gen -i ./api/spec/address/open-api-specification.json -c ./api/config/address/ng-swagger-gen.json -o ./src/app/api/generated/address",
    "generate-film-client": "ng-swagger-gen -i ./api/spec/film/open-api-specification.json -c ./api/config/film/ng-swagger-gen.json -o ./src/app/api/generated/film",
    "generate-payment-client": "ng-swagger-gen -i ./api/spec/payment/open-api-specification.json -c ./api/config/payment/ng-swagger-gen.json -o ./src/app/api/generated/payment",
    "generate-store-client": "ng-swagger-gen -i ./api/spec/store/open-api-specification.json -c ./api/config/store/ng-swagger-gen.json -o ./src/app/api/generated/store"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-swagger-gen": "^1.8.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
